{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "Firefox ESR",
            "not ie <= 11"
          ]
        },
        "useBuiltIns": true,
        "debug": true
      }
    ],
    "react"
  ],
  "env": {
    "production": {
      "plugins": [["emotion", { "sourceMap": false }]]
    },
    "development": {
      "plugins": [["emotion", { "sourceMap": true }]]
    }
  },
  "plugins": [
    "emotion",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-es2015-destructuring"
  ]
}
